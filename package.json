{
  "name": "odoo-extension",
  "version": "1.0.0",
  "description": "My odoo client",
  "main": "index.js",
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1",
    "start": "ts-node src/server/server.tsx",
    "build": "npm run build-ts && npm run tslint && npm run cp-assets && webpack --config webpack.dev.js",
    "build:dist": "tsc -p tsconfig.prod.json && tsc && npm run cp-assets && webpack --config webpack.prod.js --env=prod --progress --profile --colors",
    "build-ts": "tsc",
    "watch-ts": "TSC_NONPOLLING_WATCHER='1' tsc -w",
    "webpack-watch": "webpack --config webpack.dev.js --watch",
    "watch-node": "nodemon artifacts/server/server.js",
    "watch": "concurrently -k -p \"[{name}]\" -n \"TypeScript,Webpack\" -c \"cyan.bold,green.bold\" \"npm run watch-ts\" \"node proxy\"",
    "tslint": "tslint -c tslint.json -p tsconfig.json",
    "clean": "rm -r dist/* && rm -r artifacts/*",
    "compile-mig": "tsc --p mig-tsconfig.json",
    "clean-mig": "rm -r migrations/compiled/*",
    "recompile-mig": "npm run clean-mig && npm run compile-mig",
    "rebuild": "npm run clean && npm run build",
    "cp-assets": "ts-node copyStaticAssets.ts",
    "webpack":"webpack --config webpack.dev.js",
    "webpack:dist":"webpack --config webpack.prod.js --env=prod --progress --profile --colors",
    "postinstall": "sh postinstall.sh"
  },
  "author": "",
  "license": "ISC",
  "dependencies": {
    "@bugsnag/js": "^5.2.0",
    "@bugsnag/plugin-express": "^5.2.0",
    "@material-ui/core": "^3.9.2",
    "@material-ui/icons": "^3.0.2",
    "accounting": "^0.4.1",
    "apollo-cache-inmemory": "^1.3.12",
    "apollo-client": "^2.4.8",
    "apollo-link-error": "^1.1.7",
    "apollo-link-http": "^1.5.9",
    "apollo-link-schema": "^1.2.2",
    "apollo-server": "^2.3.1",
    "body-parser": "^1.18.3",
    "classnames": "^2.2.6",
    "cookie-parser": "^1.4.4",
    "express-device": "^0.4.2",
    "graphql": "^14.1.0",
    "graphql-tag": "^2.10.0",
    "http-proxy-middleware": "^0.19.1",
    "immutability-helper": "^2.9.1",
    "isomorphic-fetch": "^2.2.1",
    "js-date-extension": "^1.0.7",
    "jsonp": "^0.2.1",
    "jwt-simple": "^0.5.5",
    "odoo-xmlrpc": "^1.0.7",
    "passport": "^0.4.0",
    "passport-custom": "^1.0.5",
    "query-string": "^6.2.0",
    "react": "^16.7.0",
    "react-apollo": "^2.3.3",
    "react-dom": "^16.7.0",
    "react-icons": "^3.3.0",
    "react-jss": "^8.6.1",
    "react-redux": "^6.0.0",
    "react-redux-loading-bar": "^4.2.0",
    "react-responsive": "^6.1.1",
    "react-router-dom": "^4.3.1",
    "react-virtualized": "^9.21.0",
    "recyclerlistview": "^1.3.4",
    "redux": "^4.0.1",
    "redux-persist": "^5.10.0",
    "redux-persist-transform-filter": "0.0.18",
    "ts-node": "^8.0.2",
